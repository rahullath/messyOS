{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "*": ["./src/*"],
      "components/*": ["./src/components/*"],
      "lib/*": ["./src/lib/*"],
      "types/*": ["./src/types/*"]
    },
    "types": ["astro/client"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}

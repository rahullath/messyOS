{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/types/supabase.ts", "**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["./src/*"],
      "components/*": ["./src/components/*"],
      "lib/*": ["./src/lib/*"],
      "types/*": ["./src/types/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
